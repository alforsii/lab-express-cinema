<div class="movies-header">
    <h2><a href="/">Ironhack Cinema</a></h2>
    <p>Click on the movie to check the showtimes</p>
</div>
<div class="movies-container">
    {{#each movies}}
    <div class="movie-box">
        <img src={{this.image}} alt={{this.name}}>
        <h4 class="movie-title"> {{this.title}} </h4>
        <a href="/movies/movie/{{this._id}}">See more</a>
    </div>
    {{/each}}
</div>